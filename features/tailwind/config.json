{
    "name": "tailwind",
    "commands": [
        "npx expo install nativewind tailwindcss@^3.4.17 react-native-reanimated@3.16.2 react-native-safe-area-context",
        "npx tailwindcss init",
        "npx expo customize metro.config.js",
        "npx expo customize babel.config.js",
        "npx @tisf/get '@recipes-expo/features/tailwind/tailwind.config.js'",
        "npx @tisf/get '@recipes-expo/features/tailwind/metro.config.js'",
        "npx @tisf/get '@recipes-expo/features/tailwind/nativewind-env.d.ts'",
        "npx @tisf/get '@recipes-expo/features/tailwind/global.css'",
        "npx @tisf/regex '@recipes-expo/features/tailwind/globalcss.yml'",
        "yes | npx gluestack-ui init",
        "npx @tisf/get '@recipes-expo/features/tailwind/babel.config.js'"
    ]
}