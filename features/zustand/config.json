{
    "name": "feature-tailwind",
    "base": "https://raw.githubusercontent.com/t-i-f/features-expo/main/tailwind/",
    "commands": [
        "npx expo install nativewind tailwindcss@^3.4.17 react-native-reanimated@3.16.2 react-native-safe-area-context",
        "npx tailwindcss init",
        "npx expo customize metro.config.js",
        "npx expo customize babel.config.js",
        "npx @tisf/get -f 'https://raw.githubusercontent.com/t-i-f/features-expo/main/tailwind/tailwind.config.js' .",
        "npx @tisf/get -f 'https://raw.githubusercontent.com/t-i-f/features-expo/main/tailwind/metro.config.js' .",
        "npx @tisf/get -f 'https://raw.githubusercontent.com/t-i-f/features-expo/main/tailwind/babel.config.js' .",
        "npx @tisf/get -f 'https://raw.githubusercontent.com/t-i-f/features-expo/main/tailwind/nativewind-env.d.ts' .",
        "npx @tisf/get -f 'https://raw.githubusercontent.com/t-i-f/features-expo/main/tailwind/global.css' .",
        "npx @tisf/regex 'https://raw.githubusercontent.com/t-i-f/features-expo/main/tailwind/globalcss.yml'",
        "npx gluestack-ui init"
    ]
}